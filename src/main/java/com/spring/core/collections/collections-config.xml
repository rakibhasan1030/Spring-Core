<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context.xsd">

	<!-- first bean -->
	<bean class="com.spring.core.collections.Employee" name="employeeOne">
		<property name="name">
			<value>Rakib</value>
		</property>
		<property name="phoneNumbers">
			<set>
				<value>01717464661</value>
				<value>01354548746</value>
			</set>
		</property>
		<property name="addresses">
			<list>
				<value>Dhaka</value>
				<value>Lalmonirhat</value>
			</list>
		</property>
		<property name="courses">
			<map>
				<entry key="Java" value="4 month"/>
				<entry key="Python" value="3 month"/>
			</map>
		</property>
	</bean>

	<!-- second bean -->
	<bean class="com.spring.core.collections.Employee" name="employeeTwo">
		<property name="name">
			<value>Hasan</value>
		</property>
		<property name="phoneNumbers">
			<set>
				<value>01658454574</value>
				<value>01846543215</value>
			</set>
		</property>
		<property name="addresses">
			<list>
				<value>Rangpur</value>
				<value>Patgram</value>
			</list>
		</property>
		<property name="courses">
			<map>
				<entry key="Android" value="6 month"/>
				<entry key="Kotlin" value="3 month"/>
			</map>
		</property>
	</bean>

</beans>
